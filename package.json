{
  "name": "rdstation",
  "version": "1.1.3",
  "description": "RDStation Javascript Integration Library for current RD API",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-class-properties"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glauberportella/rdstation-js.git"
  },
  "keywords": [
    "RD",
    "leads",
    "marketing",
    "library",
    "integration",
    "digital"
  ],
  "author": "Glauber Portella <glauberportella@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glauberportella/rdstation-js/issues"
  },
  "homepage": "https://github.com/glauberportella/rdstation-js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0"
  }
}
